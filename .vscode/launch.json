{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "FastAPI (Uvicorn)",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "main:app",
        "--reload",
        "--port",
        "8081",
        "--app-dir",
        "src"
      ],
      "jinja": true,
      "justMyCode": true,
      "console": "integratedTerminal"
    }
  ]
}
